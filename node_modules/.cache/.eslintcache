ult Cart;\r\n","C:\\Users\\asus\\Desktop\\amzon\\E-shopping-cart\\src\\data.js",[],[],"C:\\Users\\asus\\Desktop\\amzon\\E-shopping-cart\\src\\components\\Cards.js",["41"],[],"import React from 'react';\r\nimport '../styles/cards.css';\r\n\r\nconst Cards = ({ item, handleClick, setList }) => {\r\n  const { title, price, img} = item;\r\n\r\n\r\n  \r\n  const handleBuy = () => {\r\n    handleClick(item);\r\n    setList((prevList) =>\r\n      prevList.map((prevItem) =>\r\n        prevItem.id === item.id ? { ...prevItem, added: true } : prevItem\r\n      )\r\n    );\r\n  };\r\n\r\n  return (\r\n    <div className=\"cards\">\r\n      <div className=\"image_box\">\r\n        <img src={img} alt=\"Image\" />\r\n      </div>\r\n      <div className=\"details\">\r\n        <p>{title}</p>\r\n        <p className=\"pricetage\" style={{ fontWeight: \"bold\" }}>\r\n          Price - {price}Rs\r\n        </p>\r\n\r\n          <button onClick={handleBuy}>Buy</button>     \r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\n\r\nexport default Cards;",{"ruleId":"42","severity":1,"message":"43","line":32,"column":13,"nodeType":"44","endLine":32,"endColumn":35},{"ruleId":"45","severity":1,"message":"46","line":20,"column":9,"nodeType":"44","endLine":20,"endColumn":38},"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","jsx-a11y/img-redundant-alt","Redundant alt attribute. Screen-readers already announce `img` tags as an image. You donâ€™t need to use the words `image`, `photo,` or `picture` (or any specified custom words) in the alt prop."]